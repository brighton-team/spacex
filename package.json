{
  "name": "spacex",
  "version": "1.0.0",
  "description": "space game on React",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "dev": "webpack serve --mode development --open --hot",
    "build": "webpack --mode production",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brighton-team/spacex.git"
  },
  "author": "Brighton-team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brighton-team/spacex/issues"
  },
  "homepage": "https://github.com/brighton-team/spacex#readme",
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-plugin-styled-components": "1.12.0",
    "css-loader": "5.1.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "5.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "path": "0.12.7",
    "prettier": "2.2.1",
    "simple-git-hooks": "^2.2.0",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.18",
    "typescript": "4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "5.26.3",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "@material-ui/core": "4.11.3",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/styled-components": "5.1.9",
    "normalize.css": "8.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "6.15.5",
    "react-router-dom": "^5.2.0",
    "styled-components": "5.2.1"
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "node_modules/.bin/eslint  --max-warnings=6",
      "prettier --write"
    ]
  }
}
